      "assets/images/contact.svg",
      "assets/images/favicon.png",
      "assets/images/footer-shape-left.png",
      "assets/images/footer-shape-right.png",
      "assets/images/header-image.svg",
      "assets/images/icons/puzzle-piece.svg",
      "assets/images/logo-footer.svg",
      "assets/images/logo.svg",
      "assets/images/logos.svg",
      "assets/images/shape/shape-1.svg",
      "assets/images/shape/shape-2.svg",
      "assets/images/shape/shape-3.svg",
      "assets/images/shape/shape-4.svg",
      "assets/images/testimonials/author-01.png",
      "assets/images/user-solid.svg",
      "assets/tailwind.css",
      "favicon.ico",
      "robots.txt",
      "index.html",
      "package.json",
      "assets/vendor.map",
      "assets/vendor.css.map"
    ],
    "babel": {
      "filename": "_babel_config_.js",
      "isParallelSafe": true,
      "majorVersion": 7,
      "fileFilter": "_babel_filter_.js"
    },
    "resolvable-extensions": [
      ".wasm",
      ".mjs",
      ".js",
      ".json",
      ".hbs"
    ],
    "root-url": "/"
  },
  "dependencies": {
    "tw-elements": "^1.0.0-alpha12",
    "admin": "*",
    "documentation": "*",
    "abortcontroller-polyfill": "^1.7.3",
    "node-fetch": "^2.6.1"
  },
  "fastboot": {
    "schemaVersion": 5,
    "htmlEntrypoint": "index.html",
    "moduleWhitelist": [
      "node-fetch",
      "abortcontroller-polyfill",
      "abortcontroller-polyfill/dist/cjs-ponyfill"
    ]
  },
  "keywords": [
    "ember-addon",
    "ember-addon"
  ]
